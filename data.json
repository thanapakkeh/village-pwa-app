// data.js
const data = {
  dueData: [],
  clearData: [],
};

// โหลดข้อมูลจาก JSON
async function loadData() {
  data.dueData = await fetch('houses_with_data.json').then(res => res.json());
  data.clearData = await fetch('houses_without_data.json').then(res => res.json());
}

// โหลดข้อมูลทันทีเมื่อเปิดแอป
loadData();
